<?xml version="1.0" encoding="utf-8"?>
<android.support.percent.PercentRelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<com.example.elaine.participatorysensingproject_android.Layout.TitleLayout
		android:id="@+id/titleLayout"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentTop="true"
		>
	</com.example.elaine.participatorysensingproject_android.Layout.TitleLayout>

	<com.example.elaine.participatorysensingproject_android.Layout.MenuLayout
		android:id="@+id/menuLayout"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentBottom="true">
	</com.example.elaine.participatorysensingproject_android.Layout.MenuLayout>



    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/refresh_layout"
        xmlns:cube_ptr="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/menuLayout"
        android:layout_below="@id/titleLayout">



	<com.example.participatorysensing.firstpage.DefinedScrollView
		android:id="@+id/definedview"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		>
	</com.example.participatorysensing.firstpage.DefinedScrollView>

    </android.support.v4.widget.SwipeRefreshLayout>



</android.support.percent.PercentRelativeLayout>